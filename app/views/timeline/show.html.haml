.timeline
  .allWrapper
    .container1
      .menu-bar
        .menu-items1
          .menu-icon1
            = image_tag "3-trend_icon.png", class:"trend_icon1" , alt:"トレンド"
          .menu-item1
            .item1-name
              = link_to("トレンド", trend_path, class: 'articles_font_trend2')
              //=link
        .menu-items2
          //= link_to URL
          .menu-icon2
            = image_tag "2-menubar-timeline.png",  class:"trend_icon1" , alt: "タイムライン"
          .menu-item2
            .item2-name
              = link_to("タイムライン", timeline_path, class: 'articles_font_timeline2')

        .menu-items3
          //= link_to URL
          .menu-icon3
            = image_tag "1-menu-bar-tag_feed.png", class:"trend_icon1" , alt:"タグフィード"
          .menu-item3
            .item3-name
              = link_to("タグフィード", tag_feed_path, class: 'articles_font_tag-feed2')
      .main-view
        .main-item
          .main-header
            .main-label-icon1
              = image_tag "2-main-view.png", class:"main-view2" , alt:"タイムラインビュー"
              .main-label1
                .label-article
                  タイムライン
        - @article.each do |article|
          .articles
            .content
              .item_header
                .item_header_start
                  .goods_icon
                    /= image_tag "goods.2.png",  class:"goods2" , alt: "いいね"
                    ◆
                  .item_content_likelist
                    = image_tag "profile.jpg", class:"timeline_avatar", alt:""
                    .action_type
                      = link_to article.user.name, "/#{article.user.name}", class: "author-name"
                      が　いいね　しました。
                      .header_action

                .item_haeder_end
                  .time_article
                    .time
                      = "#{article.created_at.strftime('%-m月%d日')}"

              .articles_content
                .user_iamge
                  = image_tag "profile.jpg", class:"timeline_avatar", alt:""
                .articles_body
                  .articles_title
                    = link_to article.title, "/articles/#{article.id}", class: "title-text"

